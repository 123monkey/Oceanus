<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.bj58.oceanus.plugins.hibernate.entity.UserDynamic" table="t_userdynamic">
        <id name="uid" type="long">
            <column name="uid" />
        </id>
        <property name="pwds" type="java.lang.String">
            <column name="pwds" />
        </property>
        <property name="version" type="java.lang.String">
            <column name="ver" />
        </property>
    </class>
</hibernate-mapping>